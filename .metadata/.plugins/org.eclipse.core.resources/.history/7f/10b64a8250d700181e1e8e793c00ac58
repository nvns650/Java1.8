package com.collectionss;

import java.time.LocalDate;
import java.time.Month;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.stream.Collector;
import java.util.stream.Collectors;

import com.reduceExamples.Employee;
import com.reduceExamples.Employee.Gender;

public class ListOperations {

	public static void main(String[] args) {
		 Employee p1 = new Employee(1, "Jake", Gender.MALE, LocalDate.of(1971,
			        Month.JANUARY, 1), 2343.0);
			    Employee p2 = new Employee(2, "Jack", Gender.MALE, LocalDate.of(1970,
			        Month.JULY, 21), 7100.0);
			    Employee p3 = new Employee(3, "Jack", Gender.FEMALE, LocalDate.of(1973,
			        Month.MAY, 29), 5455.0);
			    Employee p4 = new Employee(4, "Jode", Gender.MALE, LocalDate.of(1974,
			        Month.OCTOBER, 16), 1800.0);
			    Employee p5 = new Employee(5, "Jeny", Gender.FEMALE, LocalDate.of(1975,
			        Month.DECEMBER, 13), 1234.0);
			    Employee p6 = new Employee(6, "Jason", Gender.MALE, LocalDate.of(1976,
			        Month.JUNE, 9), 3211.0);
			    

			    List<Employee> persons = Arrays.asList(p1, p2, p3, p4, p5, p6);
			    
			    List<String> ll=persons.stream().map(Employee::getName).collect(ArrayList::new, ArrayList::add, ArrayList::addAll);
			    System.out.println(ll);
			    
			    List<Employee> li=persons.stream().collect(Collectors.toList());
			    li.forEach(s -> System.out.println(s));
			    
			    List<String> li2=persons.stream().map(Employee::getName).distinct().skip(1).collect(Collectors.toList());
			    li2.forEach(s -> System.out.println(s));
			    
			    Map<String,LocalDate> li3=persons.stream().collect(Collectors.toMap(Employee::getName, Employee::getDob));
			    System.out.println(li3);
			    
			    
			    
	}

}
